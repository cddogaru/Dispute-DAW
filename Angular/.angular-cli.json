{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "my-dispute"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "logo.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "../src/assets/styles/style.css",
        "../src/assets/styles/bootstrap.min.css",
        "../node_modules/font-awesome/css/font-awesome.css"
      ],
      "scripts": [
        "../src/assets/js/jquery.js",
        "../src/assets/js/bootstrap.js",
        "../src/assets/js/bootstrap.min.js",
        "../src/assets/js/loading.js",
        "../src/assets/js/password.js",
        "../src/assets/js/dropdown.js",

        "../src/assets/js/affix.js",
        "../src/assets/js/alert.js",
        "../src/assets/js/button.js",
        "../src/assets/js/carousel.js",
        "../src/assets/js/collapse.js",
        "../src/assets/js/modal.js",
        "../src/assets/js/popover.js",
        "../src/assets/js/scrollspy.js",
        "../src/assets/js/tab.js",
        "../src/assets/js/tooltip.js",
        "../src/assets/js/transition.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
